@font-face {
  font-family: "iconfont"; /* Project id 3914555 */
  src: url('data:font/woff2;base64,') format('woff2'),
       url('data:font/woff;base64,') format('woff'),
       url('data:font/ttf;base64,') format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-shequ:before {
  content: "\e62f";
}
.icon-guanyuwomen:before {
  content: "\e601";
}
.icon-shipin_o:before {
  content: "\eb81";
}
.icon-chaoshi:before {
  content: "\e68a";
}
.home swiper {
  width: 750rpx;
  height: 380rpx;
}
.home swiper image {
  width: 100%;
  height: 100%;
}
.nav {
  display: flex;
}
.nav .nav_item {
  width: 25%;
  text-align: center;
}
.nav .nav_item view {
  width: 120rpx;
  height: 120rpx;
  background: #b50e03;
  border-radius: 60rpx;
  margin: 10px auto;
  line-height: 120rpx;
  color: #fff;
  font-size: 50rpx;
}
.nav .nav_item text {
  font-size: 32rpx;
}
.recommend {
  background: #eee;
  overflow: hidden;
  margin-top: 10rpx;
}
.recommend .title {
  height: 50px;
  line-height: 50px;
  color: #b50e03;
  text-align: center;
  background: #fff;
  letter-spacing: 20rpx;
  margin: 7rpx 0;
}
.commodity_list {
  padding: 0 15rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.commodity_list .commodity_item {
  background: #fff;
  width: 355rpx;
  margin: 10rpx 0;
  padding: 15rpx;
  box-sizing: border-box;
}
.commodity_list .commodity_item image {
  width: 80%;
  height: 150rpx;
  margin: 0 auto;
}
.commodity_list .commodity_item .price {
  color: #b50e03;
  font-size: 30rpx;
  margin: 15rpx 0;
}
.commodity_list .commodity_item .price text:nth-child(2) {
  color: #ccc;
  font-size: 25rpx;
  margin-left: 17rpx;
  text-decoration: line-through;
}
.commodity_list .commodity_item .name {
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  white-space: nowrap;
  font-size: 30rpx;
  height: 50rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
}